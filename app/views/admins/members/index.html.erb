<h1>Admins::Members#index</h1>
<div class="col-xs-9">
	<h2>会員一覧</h2>

	<table class="table">
		<thead>
			<tr>
				<th>会員ID</th>
				<th>氏名</th>
				<th>メールアドレス</th>
				<th>ステータス</th>
			</tr>
		</thead>

		<tbody>
			<% @members.each do |member| %>
			<tr>
				<td><%= member.id %></td>
				<td>
					<%= link_to admins_member_path(member) do %>
					<%= member.last_name %>
					<%= member.first_name %>
					<% end %>
				</td>
				<td><%= member.email %></td>
				<td><% if (member.is_unsubscribe_status == true) %>退会済
                <% else %>有効<% end %></td>
			</tr>
			<% end %>
		</tbody>
	</table>

</div>